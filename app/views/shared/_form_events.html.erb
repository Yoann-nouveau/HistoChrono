<%= simple_form_for [@period, @event] do |f| %>
  <%= f.input :name %>
  <%= f.input :start_day %>
  <%= f.input :start_month %>
  <%= f.input :start_year %>
  <%= f.input :end_day %>
  <%= f.input :end_month %>
  <%= f.input :end_year %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :wikipedia_url %>
  <%= f.input :youtube_url %>
  <%= f.submit 'Create an event' %>
  <% end %>
