<%= simple_form_for(@personality) do |f| %>
  <%= f.input :fullname %>
  <%= f.input :birth_day%>
  <%= f.input :birth_month%>
  <%= f.input :birth_year%>
  <%= f.input :death_day %>
  <%= f.input :death_month %>
  <%= f.input :death_year %>
  <%= f.input :description %>
  <%= f.input :wikipedia_url %>
  <%= f.submit 'Create a personality' %>
  <% end %>
