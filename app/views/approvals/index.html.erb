
<div class="container">

  <% @monuments.each do |data| %>
    <% if data.user_id != current_user.id %>
      <div class="card-center">
        <div class="card-approvals">
          <i class="fa-solid fa-landmark card-font" style="color: #526D82;"></i>
          <%= render "shared/card_approval", data: data %>
        </div>
      </div>
    <% end %>
  <% end %>

  <% @events.each do |data| %>
    <% if data.user_id != current_user.id %>
      <div class="card-center">
        <div class="card-approvals">
          <i class="fa-solid fa-circle-info card-font" style="color: #040101;"></i>
          <%= render "shared/card_approval", data: data %>
        </div>
      </div>
    <% end %>
  <% end %>

  <% @personalities.each do |data| %>
      <% if data.user_id != current_user.id %>
        <div class="card-center">
          <div class="card-approvals">
            <i class="fa-solid fa-circle-user card-font" style="color: #8B635C;"></i>
            <%= render "shared/card_approval", data: data %>
          </div>
        </div>
    <% end %>
  <% end %>

</div>
